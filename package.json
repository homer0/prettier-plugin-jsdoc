{
  "name": "@homer0/prettier-plugin-jsdoc",
  "description": "A Prettier plugin to format JSDoc blocks.",
  "version": "7.0.0",
  "repository": "homer0/prettier-plugin-jsdoc",
  "author": "Leonardo Apiwan (@homer0) <me@homer0.com>",
  "license": "MIT",
  "keywords": [
    "jsdoc",
    "prettier",
    "comments",
    "documentation",
    "javascript"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "commonjs",
  "dependencies": {
    "comment-parser": "^1.3.1",
    "prettier": "^3.0.0",
    "ramda": "0.29.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@homer0/eslint-plugin": "^10.3.2",
    "@homer0/prettier-config": "^1.1.3",
    "eslint": "^8.45.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.6.1",
    "jest-environment-node": "^29.6.1",
    "leasot": "^13.3.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0"
  },
  "engine-strict": true,
  "engines": {
    "node": ">=16"
  },
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint .",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest -c ./.jestrc-unit.js",
    "test:e2e": "NODE_OPTIONS=--experimental-vm-modules jest -c ./.jestrc-e2e.js",
    "test": "pnpm run test:unit && pnpm run test:e2e"
  }
}
