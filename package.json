{
  "name": "@homer0/prettier-plugin-jsdoc",
  "description": "A Prettier plugin to format JSDoc blocks",
  "version": "8.0.0",
  "repository": "homer0/prettier-plugin-jsdoc",
  "author": "Leonardo Apiwan (@homer0) <me@homer0.com>",
  "license": "MIT",
  "keywords": [
    "jsdoc",
    "prettier",
    "comments",
    "documentation",
    "javascript"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "commonjs",
  "dependencies": {
    "comment-parser": "^1.4.1",
    "prettier": "^3.2.5",
    "ramda": "0.29.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@homer0/eslint-plugin": "^12.0.4",
    "@homer0/prettier-config": "^1.1.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "leasot": "^13.3.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "semantic-release": "^22.0.5"
  },
  "engine-strict": true,
  "engines": {
    "node": ">=18.17 <21"
  },
  "main": "src/index.js",
  "scripts": {
    "prepare": "./utils/scripts/prepare",
    "test:unit": "./utils/scripts/test-unit",
    "test:e2e": "./utils/scripts/test-e2e",
    "test": "./utils/scripts/test",
    "lint": "./utils/scripts/lint",
    "lint:all": "./utils/scripts/lint-all",
    "todo": "./utils/scripts/todo"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write"
    ]
  }
}
